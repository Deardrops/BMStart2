{
    "name": "BMStart - Better New Tab For You",
    "version": "0.2.0",
    "description": "BMStart (stand for BookmarkStart) can replace your default NEW TAB by bookmarks view. See more: https://github.com/Deardrops/BMStart2",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "bookmarks",
        "history",
        "chrome://favicon/",
        "topSites",
        "management",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "main.html"
    },
    "background": {
        "page": "background.html"
    },
    "minimum_chrome_version": "19.0",
    "default_locale": "en",
    "manifest_version": 2,
    "web_accessible_resources": [
        "main.html"
    ]
}
